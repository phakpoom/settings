{
    "name": "phakpoom/settings",
    "description": "Just a settings library.",
    "keywords": [
        "php",
        "settings"
    ],
    "homepage": "https://github.com/phpmob/settings",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Ishmael Doss",
            "email": "nukboon@gmail.com"
        },
        {
            "name": "Bonn"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpMob\\Settings\\": ""
        },
        "classmap": []
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "require": {
        "php": ">=7.2 || >=8.0",
        "doctrine/orm": "^2.7 | ^3.0"
    },
    "require-dev": {
        "cache/filesystem-adapter": "^1.0",
        "phpunit/phpunit": "^8.0",
        "symfony/form": "^3.3",
        "symfony/validator": "^3.3"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "suggest": {
        "cache/filesystem-adapter": "To use built-in \\PhpMob\\Settings\\Manager\\CachedManager."
    }
}
